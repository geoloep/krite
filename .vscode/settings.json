{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".ts$",
                "cmd": "cd ${workspaceRoot} && tsc -d"
            },
            {
                "match": ".ts$",
                "cmd": "rsync -r -v --exclude-from ${workspaceRoot}/exclude.txt /opt/lampp/htdocs/dev/krite /opt/lampp/htdocs/dev/kadaster/node_modules"
            }
        ]
    }
}